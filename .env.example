# Pterodactyl Telegram Bot Configuration - Node.js Version
# Copy this file to .env and fill in your values

# Essential Configuration
BOT_TOKEN=your_telegram_bot_token
BOT_USERNAME=your_bot_username
OWNER_TELEGRAM_ID=your_telegram_user_id

# Pterodactyl Panel Configuration
PTERODACTYL_PANEL_URL=https://panel.yourdomain.com
PTERODACTYL_APPLICATION_API_KEY=ptla_your_application_api_key_here
PTERODACTYL_CLIENT_API_KEY=ptlc_your_client_api_key_here

# Optional Settings
LOG_LEVEL=INFO
DEBUG_MODE=false

# Author Information
AUTHOR_NAME=Pablos
AUTHOR_TELEGRAM=@ImTamaa
