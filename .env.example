# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here
BOT_USERNAME=your_bot_username
OWNER_TELEGRAM_ID=your_telegram_id

# Pterodactyl Panel Configuration
PTERODACTYL_PANEL_URL=https://your-panel-domain.com
PTERODACTYL_APPLICATION_API_KEY=ptla_your_application_api_key
PTERODACTYL_CLIENT_API_KEY=ptlc_your_client_api_key

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log
LOG_MAX_FILES=7

# Database Configuration (SQLite untuk logging)
DB_PATH=logs/bot.db

# Security Configuration
ALLOWED_USERS=your_telegram_id
MAX_CONCURRENT_OPERATIONS=10
OPERATION_TIMEOUT=300

# Bot Configuration
DEBUG_MODE=false

# Author Information
AUTHOR_NAME=Your Name
AUTHOR_TELEGRAM=@YourTelegram
